{
  "name": "react-map-challenge",
  "version": "1.0.0",
  "description": "react google map with multiples pin and a slider",
  "repository": {
    "type": "git"
  },
  "author": "Morlo Mbakop",
  "license": "UNLICENSED",
  "main": "src/index.tsx",
  "scripts": {
    "start:server": "node server",
    "start:fe": "parcel ./src/index.html",
    "start": "npm run start:server & npm run start:fe",
    "foo": "node server parcel ./src/index.html"
  },
  "engines": {
    "node": ">= 8.11.0",
    "npm": ">= 6.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/config/test-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(test|spec).(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "google-maps": "^3.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.2.1"
  },
  "devDependencies": {
    "@types/faker": "^4.1.5",
    "@types/google-maps": "^3.2.1",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/styled-components": "^4.1.15",
    "concurrently": "^4.1.0",
    "jest": "^24.3.1",
    "parcel": "^1.12.3",
    "prettier": "^1.16.4",
    "react-testing-library": "^6.0.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.5.1"
  }
}
